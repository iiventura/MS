CREATE TABLE EMPLEADO (IDEMPLEADO INTEGER AUTO_INCREMENT NOT NULL, tipoEmpleado TINYINT(1), ACTIVO TINYINT(1) default 0, DNI VARCHAR(255), DOMICILIO VARCHAR(255), NOMBRE VARCHAR(255), VERSION INTEGER, idDepartamento INTEGER, PRIMARY KEY (IDEMPLEADO))
CREATE TABLE DIRECTOR (IDEMPLEADO INTEGER NOT NULL, POLITICAEMPRESARIAL VARCHAR(255), PRIMARY KEY (IDEMPLEADO))
CREATE TABLE AGENTEINMOBILIARIO (IDEMPLEADO INTEGER NOT NULL, AÑOSEXPERIENCIA INTEGER, PRIMARY KEY (IDEMPLEADO))
CREATE TABLE DEPARTAMENTO (IDDEPARTAMENTO INTEGER AUTO_INCREMENT NOT NULL, ACTIVO TINYINT(1) default 0, CATEGORIA VARCHAR(255), NOMBRE VARCHAR(255), VERSION INTEGER, PRIMARY KEY (IDDEPARTAMENTO))
ALTER TABLE EMPLEADO ADD CONSTRAINT FK_EMPLEADO_idDepartamento FOREIGN KEY (idDepartamento) REFERENCES DEPARTAMENTO (IDDEPARTAMENTO)
ALTER TABLE DIRECTOR ADD CONSTRAINT FK_DIRECTOR_IDEMPLEADO FOREIGN KEY (IDEMPLEADO) REFERENCES EMPLEADO (IDEMPLEADO)
ALTER TABLE AGENTEINMOBILIARIO ADD CONSTRAINT FK_AGENTEINMOBILIARIO_IDEMPLEADO FOREIGN KEY (IDEMPLEADO) REFERENCES EMPLEADO (IDEMPLEADO)
